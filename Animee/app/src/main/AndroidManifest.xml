<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.animee">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/star"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/TranslucentTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".home.GuideActivity"></activity>
        <activity android:name=".home.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LuckAnalysisActivity" />
        <activity android:name=".PartnerAnalysisActivity" />
        <activity android:name=".StarDetailActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>